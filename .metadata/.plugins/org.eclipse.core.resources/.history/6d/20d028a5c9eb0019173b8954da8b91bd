package aplicacoes;

import entidades.HerancaContaBancaria;
import entidades.HerancaContaBancariaEmpresa;

public class UpcastingDowncasting {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		HerancaContaBancaria conta = new HerancaContaBancaria(2000, "Zé das coves", 150.3);
		HerancaContaBancariaEmpresa contaEmpresa = new HerancaContaBancariaEmpresa(3000, "joelma", 20.6, 5.4);
		
		// UpCasting
		// Nestes dois casos não deu erro porque estamos atribuindo uma subClasse a uma Classe, isto é UpCasting
		HerancaContaBancaria contaTeste = contaEmpresa;
		HerancaContaBancaria conta2 = new HerancaContaBancariaEmpresa(4000, "zé do teste", 34.8, 22.3); 
	}

}
